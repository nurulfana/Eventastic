        <activity
            android:name=".EGuestCrew.Guest.GuestCrewInsertGuest"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".EGuestCrew.Crew.GuestCrewInsertCrew"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".EGuestCrew.GuestCrew"
            android:label="@string/title_activity_guest_crew"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".GAds.AdsActivity"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
